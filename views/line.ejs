<input type="date" class="datepicker">

<div id="graph" width="100%" ></div>

<script src="/js/ticket/line.js"></script>

<script>
	$(document).ready(function() {
		$.ajax({
			type:"POST",
			url: '/commit'
		}).then(
			function (data) {
				drawLineChart(data);
			},
			function () {
				alert("読み込み失敗");
			}
		);
	});

